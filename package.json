{
  "name": "site-switcher",
  "version": "0.2.0",
  "description": "Web and Electron apps to display multiple websites (one after another).",
  "main": "src/main.jsx",
  "scripts": {
    "lint": "eslint src/*.js src/**/*.js src/*.jsx src/**/*.jsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "web:dev": "webpack-dev-server -d --hot --inline --progress --colors",
    "web:build": "webpack --progress --colors",
    "web:serve": "http-server ./static -p 8090",
    "app:install": "cd electron-app && npm install",
    "app:prepare": "rm -rf electron-app/dist && mkdir electron-app/dist",
    "app:dev": "npm run app:prepare && webpack --watch --progress --colors --config webpack.electron.js",
    "app:run": "./node_modules/.bin/electron electron-app",
    "app:prepare:build": "npm run app:prepare && webpack --progress --colors --config webpack.electron.js",
    "app:build:linux": "npm run app:prepare:build && cd electron-app && ./node_modules/.bin/build --linux",
    "app:build:arm": "npm run app:prepare:build && cd electron-app && ./node_modules/.bin/build --armv7l"
  },
  "author": "https://github.com/jacekk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jacekk/site-switcher"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/init.js",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/src/store|material",
      "/node_modules/",
      "/tests/"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.25.0",
    "dotenv-webpack": "^1.3.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.7.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.3.0",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "istanbul": "^0.4.5",
    "jest-cli": "^18.1.0",
    "jquery": "^3.1.1",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "node-localstorage": "^1.3.0",
    "react-addons-test-utils": "^15.4.2",
    "sessionstorage": "0.0.2",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "fbjs": "^0.8.5",
    "material-ui": "^0.16.0",
    "react": "^15.3.2",
    "react-addons-create-fragment": "^15.3.2",
    "react-addons-pure-render-mixin": "^15.3.2",
    "react-addons-transition-group": "^15.3.2",
    "react-addons-update": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^2.0.0",
    "redux": "^3.6.0",
    "redux-localstorage": "^0.4.1",
    "uuid": "^2.0.3",
    "validator": "^6.1.0"
  }
}
