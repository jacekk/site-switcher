{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    },
    "react": {
        "version": "15.3"
    }
  },
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "comma-spacing": 0,
    "consistent-return": 0,
    "curly": 0,
    "eqeqeq": [2, "allow-null"],
    "key-spacing": 0,
    "new-cap": 0,
    "import/no-duplicates": 2,
    "no-extend-native": 0,
    "no-multiple-empty-lines": [2],
    "no-shadow-restricted-names": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-var": 2,
    "quotes": 0,
    "react/no-string-refs": [1],
    "semi-spacing": 0,
    "space-infix-ops": 0,
    "space-unary-ops": 0,
    "strict": 0
  },
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "react"
  ]
}
